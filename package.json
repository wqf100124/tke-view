{
  "name": "tke-view",
  "version": "2.0.2",
  "description": "View local development environment.",
  "main": "index.js",
  "author": "rt.wadewang@rayootech.com",
  "license": "ISC",
  "repository": "git@github.com:wqf100124/tke-view.git",
  "type": "module",
  "scripts": {
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "build:ubuntu": "docker build -t registry.cn-hangzhou.aliyuncs.com/tke-view/view:2.0.2 --no-cache view/2.0/ubuntu",
    "build:alpine": "docker build -t registry.cn-hangzhou.aliyuncs.com/tke-view/view:2.0.2-alpine --no-cache view/2.0/alpine",
    "build:ubuntu-3.0.1": "docker build -t registry.cn-hangzhou.aliyuncs.com/tke-view/view:3.0.1 --no-cache view/3.0/ubuntu",
    "build:alpine-3.0.1": "docker build -t registry.cn-hangzhou.aliyuncs.com/tke-view/view:3.0.1-alpine -t registry.cn-hangzhou.aliyuncs.com/tke-view/view:alpine --no-cache view/3.0/alpine",
    "build:ubuntu-4.0.0": "docker build -t registry.cn-hangzhou.aliyuncs.com/tke-view/view:4.0.0 -t registry.cn-hangzhou.aliyuncs.com/tke-view/view:latest --no-cache view/4.0/ubuntu",
    "build:alpine-4.0.0": "docker build -t registry.cn-hangzhou.aliyuncs.com/tke-view/view:4.0.0-alpine -t registry.cn-hangzhou.aliyuncs.com/tke-view/view:alpine --no-cache view/4.0/alpine",
    "build:autotest": "docker build -t rtwadewang/autotest:1.0.3 -t rtwadewang/autotest:latest --no-cache ./autotest"
  },
  "devDependencies": {
    "@mermaid-js/mermaid-mindmap": "^9.3.0",
    "echarts": "^5.5.1",
    "vitepress": "^1.5.0",
    "vitepress-plugin-mermaid": "^2.0.17",
    "vue": "^3.5.13"
  },
  "dependencies": {
    "mermaid": "11.4.1"
  }
}
